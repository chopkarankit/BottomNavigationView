            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp">
        </TextView>

        <TextView
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/_00_00_00"
            android:textColor="#000">
        </TextView>
    </LinearLayout>

    <LinearLayout
