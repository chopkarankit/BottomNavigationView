            android:layout_width="200dp"
            android:layout_height="wrap_content"
            tools:ignore="UselessParent">

            <androidx.cardview.widget.CardView
                android:id="@+id/fl1"
                android:layout_width="35dp"
                android:layout_height="35dp"
                app:cardCornerRadius="30dp"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                app:cardBackgroundColor="#E91E63">

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/crown"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="5dp"
                    android:contentDescription="@string/todoslpabr"
                    app:tint="#ffffff">
                </ImageView>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="120dp"
                android:layout_height="30dp"
                app:cardCornerRadius="13dp"
                app:cardBackgroundColor="#ED6C98"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@id/fl1"
                android:layout_toEndOf="@id/fl1"
                android:layout_marginStart="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/week_top_4_10"
                    android:textColor="#ffffff"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginStart="8dp">
                </TextView>
            </androidx.cardview.widget.CardView>
        </RelativeLayout>

        <RelativeLayout
            android:layout_marginTop="8dp"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            tools:ignore="UselessParent">

            <androidx.cardview.widget.CardView
                android:id="@+id/fl2"
                android:layout_width="35dp"
                android:layout_height="35dp"
                app:cardCornerRadius="30dp"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                app:cardBackgroundColor="#E91E63">

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/crown"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="5dp"
                    android:contentDescription="@string/todoslpabr"
                    app:tint="#ffffff">
                </ImageView>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="120dp"
                android:layout_height="30dp"
                app:cardCornerRadius="13dp"
                app:cardBackgroundColor="#ED6C98"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@id/fl2"
                android:layout_toEndOf="@id/fl2"
                android:layout_marginStart="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/week_top_2_3kls"
                    android:textColor="#ffffff"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginStart="8dp">
                </TextView>
            </androidx.cardview.widget.CardView>
        </RelativeLayout>

        <RelativeLayout
