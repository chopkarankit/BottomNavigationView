                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@drawable/diamond"
                    app:tint="#FFFF00"
                    android:layout_marginTop="3dp"
                    android:contentDescription="@string/tododgueb">
                </ImageView>

                <TextView
