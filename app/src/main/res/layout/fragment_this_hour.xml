                android:layout_height="25dp"
                android:src="@drawable/medal"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:contentDescription="@string/todomdl1">
            </ImageView>
            

        </LinearLayout>

        <LinearLayout
