                android:layout_marginTop="10dp"
                android:layout_marginRight="30dp"
                android:layout_marginEnd="30dp"
                app:tint="#ED3372"
                android:contentDescription="@string/todom">
            </ImageView>
        </RelativeLayout>
    </LinearLayout>

    <View
        android:id="@+id/fth14"
        android:layout_below="@+id/fth11"
